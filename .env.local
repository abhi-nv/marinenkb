
# Example environment variables
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration (Demo URLs - replace with your actual Supabase project)
VITE_SUPABASE_URL=https://demo-marine-data.supabase.co
VITE_SUPABASE_ANON_KEY=demo_key_replace_with_actual_supabase_anon_key

# API Configuration
VITE_API_URL=http://localhost:8001

# Application Configuration
VITE_APP_NAME=Marine Data Verse
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_REAL_TIME=true
VITE_DEBUG=true

# External Services
VITE_MAPBOX_TOKEN=your_mapbox_access_token
VITE_ANALYTICS_ID=your_analytics_id

# Security
VITE_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

fetch(`${import.meta.env.VITE_API_URL}/api/marine-data`)

-- Create the database
CREATE DATABASE marine_data_verse;

-- Connect to the new database
\c marine_data_verse

-- Create the table
CREATE TABLE marine_data (
  id SERIAL PRIMARY KEY,
  name VARCHAR(100),
  type VARCHAR(50),
  location VARCHAR(100),
  observed_at TIMESTAMP
);

npm run dev